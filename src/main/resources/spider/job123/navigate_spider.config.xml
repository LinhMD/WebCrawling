<?xml version="1.0" encoding="UTF-8"?>
<Spider id="NavSpider">
    <SpiderJobs>
        <Visit>
            <url>https://123job.vn/jobs</url>
            <method>start</method>
        </Visit>
        <Tag>
            <tagName>a</tagName>
            <method>findAll</method>
        </Tag>
        <AddUrlToQueue>
            <queueName>job123</queueName>
            <addIfMissingUrl>https://123job.vn</addIfMissingUrl>
            <urlStartWiths>
                <url>https://123job.vn/viec-lam</url>
                <url>https://123job.vn/jobs</url>
                <url>https://123job.vn/việc-làm-</url>
                <url>https://123job.vn/t/</url>
            </urlStartWiths>
        </AddUrlToQueue>
        <Repeat>
            <interval>100</interval>
            <SpiderJobs>
                <VisitFromUrlQueue>
                    <method>start</method>
                    <queueName>job123</queueName>
                    <urlStartWiths>
                        <url>https://123job.vn/jobs</url>
                        <url>https://123job.vn/việc-làm-</url>
                        <url>https://123job.vn/t/</url>
                    </urlStartWiths>
                </VisitFromUrlQueue>
                <Tag>
                    <tagName>a</tagName>
                    <method>findAll</method>
                </Tag>
                <AddUrlToQueue>
                    <queueName>job123</queueName>
                    <addIfMissingUrl>https://123job.vn</addIfMissingUrl>
                    <urlStartWiths>
                        <url>https://123job.vn/viec-lam</url>
                        <url>https://123job.vn/jobs</url>
                        <url>https://123job.vn/việc-làm-</url>
                        <url>https://123job.vn/t/</url>
                    </urlStartWiths>
                </AddUrlToQueue>
            </SpiderJobs>
        </Repeat>
    </SpiderJobs>
</Spider>
